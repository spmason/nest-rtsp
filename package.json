    "basic-auth": "^2.0.1",
    "better-sqlite3": "^7.5.3",
    "canvas": "^2.9.1",
    "cli-color": "^2.0.2",
    "debug": "^4.3.4",
    "dot-object": "^2.1.4",
    "dotenv": "^16.0.1",
    "express": "^4.18.1",
    "find-open-port": "^2.0.3",
    "googleapis": "^100.0.0",
